{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express, and mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": "last 2 versions",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "vercel-build": "echo hello"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 0,
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "req|res|next|val"
        }
      ],
      "import/no-extraneous-dependencies": 0,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "endOfLine": "auto",
          "tabWidth": 4,
          "printWidth": 80
        }
      ]
    }
  },
  "author": "Beefy",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^3.16.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.5.0",
    "mongoose": "^6.13.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "pug": "^3.0.2",
    "sharp": "^0.32.2",
    "slugify": "^1.6.6",
    "stripe": "^7.0.0",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "eslint-config-wesbos": "^3.2.3"
  }
}
